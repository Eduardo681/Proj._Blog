<%- include ../../partials/header.ejs %>
<%- include ../../partials/navbar.ejs %>
<body> 
    <div class="container">
        <hr>
        <h2>Usúarios</h2>
        <a class="btn btn-primary" href="/admin/users/create">Criar novo usúario</a>
        <hr>
        <table class="table table-bordered">
            <thead>
                <th>ID</th>
                <th>Email</th>
            </thead>
            <tbody>
                <%users.forEach(user=>{%>
                <tr>
                <td><%=user.id%></td>
                <td><%=user.email%></td>

                </tr>
                <%})%>
            </tbody>
        </table>
    </div>

</body>
